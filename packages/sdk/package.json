		"test": "vitest"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@kwenta/sdk": "^1.2.0",
		"hono": "^4.5.9",
		"viem": "^2.21.6"
	},
	"devDependencies": {
		"tsx": "^4.19.1",
		"vitest": "^2.1.1",
		"orderbook-backend": "workspace:*"
	}
