{
	"name": "orderbook-sdk",
	"version": "1.0.0",
	"description": "SDK for interacting with the Synthetix V3 Orderbook",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsc --build",
		"dev": "tsc --watch",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -rf dist",
